<VBox prefHeight="600.0" prefWidth="800.0" fx:id="root"
    xmlns="http://javafx.com/javafx/20.0.1"
    xmlns:fx="http://javafx.com/fxml/1" fx:controller="cardgame.controller.RoomPickController">

    <HBox alignment="CENTER" spacing="10" prefHeight="50">
        <Label text="Pick a room" styleClass="big_label" />
    </HBox>

    <HBox alignment="CENTER" spacing="10">
        <HBox fx:id="room1" alignment="CENTER" spacing="10" styleClass="room" prefWidth="250" prefHeight="200"/>
        <HBox fx:id="room2" alignment="CENTER" spacing="10" styleClass="room" prefWidth="250" prefHeight="200"/>
        <HBox fx:id="room3" alignment="CENTER" spacing="10" styleClass="room" prefWidth="250" prefHeight="200"/>
    </HBox>

</VBox>